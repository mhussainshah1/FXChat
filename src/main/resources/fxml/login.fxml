      <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Proxy Host  (Socks)" GridPane.rowIndex="4" />
      <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Proxy Port (Socks)" GridPane.rowIndex="5" />
      <Button mnemonicParsing="false" onAction="#actionHandler" prefHeight="25.0" prefWidth="61.0" text="Connect" GridPane.rowIndex="6">
