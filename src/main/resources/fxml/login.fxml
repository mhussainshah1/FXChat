      <TextField fx:id="txtUserName" GridPane.columnIndex="1">
          <GridPane.margin>
              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </GridPane.margin>
      </TextField>
      <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" text="Nick Name" textAlignment="CENTER">
