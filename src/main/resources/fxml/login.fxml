              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </GridPane.margin>
      </TextField>
      <CheckBox fx:id="proxyCheckBox" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="3">
          <GridPane.margin>
              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </GridPane.margin>
      </CheckBox>
  </GridPane>