              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </GridPane.margin>
      </Button>
      <TextField fx:id="txtServerName" GridPane.columnIndex="1" GridPane.rowIndex="1">
          <GridPane.margin>
              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
