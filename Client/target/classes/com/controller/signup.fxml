<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="250.0"
          style="-fx-background-color: E0ECFEFF;" xmlns="http://javafx.com/javafx/17.0.2-ea"
          fx:controller="com.controller.SignUpController">

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>

    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Nick Name"/>
    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Password" GridPane.rowIndex="1"/>
    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Room" GridPane.rowIndex="2"/>
    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Server Name" GridPane.rowIndex="3"/>
    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Server Port" GridPane.rowIndex="4"/>
    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Proxy" GridPane.rowIndex="5"/>
    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Proxy Host  (Socks)" GridPane.rowIndex="6"/>
    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Proxy Port (Socks)" GridPane.rowIndex="7"/>

    <TextField fx:id="txtUserName" text="moe" GridPane.columnIndex="1">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </TextField>
    <PasswordField fx:id="txtPassword" text="password" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </PasswordField>
    <ChoiceBox fx:id="choiceRoom" onAction="#actionHandler" value="General" GridPane.columnIndex="1"
               GridPane.rowIndex="2">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </ChoiceBox>
    <TextField fx:id="txtServerName" text="localhost" GridPane.columnIndex="1" GridPane.rowIndex="3">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </TextField>
    <TextField fx:id="txtServerPort" text="1436" GridPane.columnIndex="1" GridPane.rowIndex="4">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </TextField>
    <CheckBox fx:id="proxyCheckBox" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="5">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </CheckBox>
    <TextField fx:id="txtProxyHost" text="51.38.28.127" GridPane.columnIndex="1" GridPane.rowIndex="6">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </TextField>
    <TextField fx:id="txtProxyPort" text="80" GridPane.columnIndex="1" GridPane.rowIndex="7">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </TextField>
    <Button mnemonicParsing="false" onAction="#actionHandler" prefHeight="25.0" prefWidth="65.0" text="Signup"
            GridPane.rowIndex="8">
        <GridPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
        </GridPane.margin>
    </Button>
    <Button cancelButton="true" mnemonicParsing="false" onAction="#actionHandler" prefHeight="25.0" prefWidth="65.0"
            text="Quit" GridPane.columnIndex="1" GridPane.rowIndex="8">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </Button>
    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Already a member?" GridPane.rowIndex="9">
        <font>
            <Font name="Arial Rounded MT Bold" size="12.0"/>
        </font>
    </Label>
    <Button fx:id="button_log_in" mnemonicParsing="false" onAction="#actionHandler" prefHeight="25.0" prefWidth="65.0"
            style="-fx-background-color: transparent; -fx-cursor: hand;" text="Log In!" textFill="#0b40d2"
            underline="true" GridPane.columnIndex="1" GridPane.rowIndex="9">
        <font>
            <Font name="Arial Rounded MT Bold" size="12.0"/>
        </font>
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </Button>
</GridPane>
